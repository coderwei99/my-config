	// }

	"react-tsx": {
		"prefix": "tsreact",
		"body": [
			"import React, { memo, ReactNode, FC } from \"react\"",
			"",
			"interface Iprops {",
			"  children?: ReactNode",
			"}",
			"",
			"const ${1:Template}: FC<Iprops> = memo(() => {",
			"  return <div>${1:Template}</div>",
			"})",
			"",
			"export default ${1:Template}",
			""
		],
		"description": "react-tsx"
	},
	"clog":{
		"prefix": "clog",
		"body": [
			"console.log(${1})",
		],
		"description": "log"
	}
}